{
  "version": 2,
  "name": "nextjs-express-nowv2",
  "builds": [
    {
      "src": "server/routes/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "client/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/routes/api"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}